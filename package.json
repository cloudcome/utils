{
  "name": "@cloudcome/utils",
  "version": "1.0.0",
  "description": "我的个人工具函数库",
  "scripts": {
    "prepare": "lefthook install",
    "lint": "biome check",
    "lint:fix": "biome check --fix --unsafe",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "cross-env NODE_ENV=production vite build",
    "generate": "node scripts/generate.cjs"
  },
  "engines": {
    "node": ">=22"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./array": {
      "types": "./dist/array.d.ts",
      "import": "./dist/array.mjs",
      "require": "./dist/array.cjs"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.mjs",
      "require": "./dist/async.cjs"
    },
    "./color": {
      "types": "./dist/color.d.ts",
      "import": "./dist/color.mjs",
      "require": "./dist/color.cjs"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.mjs",
      "require": "./dist/date.cjs"
    },
    "./dict": {
      "types": "./dist/dict.d.ts",
      "import": "./dist/dict.mjs",
      "require": "./dist/dict.cjs"
    },
    "./easing": {
      "types": "./dist/easing.d.ts",
      "import": "./dist/easing.mjs",
      "require": "./dist/easing.cjs"
    },
    "./fn": {
      "types": "./dist/fn.d.ts",
      "import": "./dist/fn.mjs",
      "require": "./dist/fn.cjs"
    },
    "./is": {
      "types": "./dist/is.d.ts",
      "import": "./dist/is.mjs",
      "require": "./dist/is.cjs"
    },
    "./number": {
      "types": "./dist/number.d.ts",
      "import": "./dist/number.mjs",
      "require": "./dist/number.cjs"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "import": "./dist/object.mjs",
      "require": "./dist/object.cjs"
    },
    "./path": {
      "types": "./dist/path.d.ts",
      "import": "./dist/path.mjs",
      "require": "./dist/path.cjs"
    },
    "./promise": {
      "types": "./dist/promise.d.ts",
      "import": "./dist/promise.mjs",
      "require": "./dist/promise.cjs"
    },
    "./qs": {
      "types": "./dist/qs.d.ts",
      "import": "./dist/qs.mjs",
      "require": "./dist/qs.cjs"
    },
    "./regexp": {
      "types": "./dist/regexp.d.ts",
      "import": "./dist/regexp.mjs",
      "require": "./dist/regexp.cjs"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.mjs",
      "require": "./dist/string.cjs"
    },
    "./tree": {
      "types": "./dist/tree.d.ts",
      "import": "./dist/tree.mjs",
      "require": "./dist/tree.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "array": [
        "./dist/array.d.ts"
      ],
      "async": [
        "./dist/async.d.ts"
      ],
      "color": [
        "./dist/color.d.ts"
      ],
      "date": [
        "./dist/date.d.ts"
      ],
      "dict": [
        "./dist/dict.d.ts"
      ],
      "easing": [
        "./dist/easing.d.ts"
      ],
      "fn": [
        "./dist/fn.d.ts"
      ],
      "is": [
        "./dist/is.d.ts"
      ],
      "number": [
        "./dist/number.d.ts"
      ],
      "object": [
        "./dist/object.d.ts"
      ],
      "path": [
        "./dist/path.d.ts"
      ],
      "promise": [
        "./dist/promise.d.ts"
      ],
      "qs": [
        "./dist/qs.d.ts"
      ],
      "regexp": [
        "./dist/regexp.d.ts"
      ],
      "string": [
        "./dist/string.d.ts"
      ],
      "tree": [
        "./dist/tree.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/cloudcome/utils"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@types/node": "22",
    "@vitest/coverage-v8": "^3.0.9",
    "commit-and-tag-version": "^12.5.0",
    "cross-env": "^7.0.3",
    "lefthook": "^1.11.5",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  }
}
