{
  "name": "@cloudcome/vue-composables",
  "version": "1.3.0",
  "description": "cloudcome vue composables",
  "scripts": {
    "lint": "biome check",
    "lint:fix": "biome check --fix --unsafe",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "cross-env NODE_ENV=production vite build",
    "generate": "node ../helpers/bin/index.cjs generate"
  },
  "engines": {
    "node": ">=22"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {},
  "typesVersions": {
    "*": {}
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/cloudcome/utils/packages/vue-composables"
  },
  "dependencies": {
    "@cloudcome/core-utils": "workspace:^",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22",
    "@vitest/coverage-v8": "^3.1.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
